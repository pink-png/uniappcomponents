				"mp-alipay": {
					"allowsBounceVertical": "NO" // 将回弹属性关掉
				}
			}
		}, {
			"path": "pages/zujian/zujian",
			"style": {
				"navigationBarTitleText": "组件",
				"enablePullDownRefresh": false
			}
		},
		
		// 组件的路径
		{
			"path": "pages/component/GsqHeader",
